package com.skp.opx.sns.entity;

/**
 * @설명 : 폰북 등록 API 사용을 위한 Payload Entity
 * @클래스명 : EntityUploadPhoneBookPayload
 */
public class EntityUploadPhoneBookPayload {

	public String nationDialing;	//국가코드 (한국 : 82)
	public String phoneNumber;	//전화번호
	public String name;				//이름
	
	public EntityUploadPhoneBookPayload(String phoneNumber, String name) {
		
		this.nationDialing = "82";
		this.phoneNumber = phoneNumber;
		this.name = name;
	}
	
	public EntityUploadPhoneBookPayload(String nationDialing, String phoneNumber, String name) {
		
		this.nationDialing = nationDialing;
		this.phoneNumber = phoneNumber;
		this.name = name;
	}
}

